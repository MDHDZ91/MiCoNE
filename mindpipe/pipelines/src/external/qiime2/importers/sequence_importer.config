sequence_importer {
    input_type = "{{ sequence_importer['input_type'] }}"
}

params {
    output_dir = "{{ output_dir }}"
    input_folder = "{{ input['qiime2_sequencedir'] }}"

    input_type = sequence_importer.input_type
}

report {
    enabled = true
    file = "${params.output_dir}/report.html"
}

timeline {
    enabled = true
    file = "${params.output_dir}/timeline.html"
}

trace {
    enabled = true
    file = "${params.output_dir}/trace.txt"
}