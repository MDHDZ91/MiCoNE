export {
    name = "{{ export['name'] }}"
}

params {
    output_dir = "{{ output_dir }}"
    artifacts = "{{ input['qiime2_artifact'] }}"

    name = export.name
}

report {
    enabled = true
    file = "${params.output_dir}/report.html"
}

timeline {
    enabled = true
    file = "${params.output_dir}/timeline.html"
}

trace {
    enabled = true
    file = "${params.output_dir}/trace.txt"
}